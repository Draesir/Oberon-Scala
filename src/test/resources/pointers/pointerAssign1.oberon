MODULE pointerAssign;

VAR
  a : POINTER TO INTEGER;
  b : POINTER TO REAL;
  c : POINTER TO CHAR;
  d : POINTER TO BOOLEAN;
  e : POINTER TO STRING;

BEGIN
    NEW(a);
    NEW(b);
    NEW(c);
    NEW(d);
    NEW(e);
    a^ := 1;
    b^ := 9.5;
    c^ := 'c';
    d^ := True;
    e^ := "Hello."
END

END pointerAssign.